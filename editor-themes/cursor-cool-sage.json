{
  "name": "Japandi Cool Sage",
  "type": "light",
  "colors": {
    "editor.background": "#F7F8F6",
    "editor.foreground": "#434B3F",
    "editor.lineHighlightBackground": "#F0F2ED",
    "editor.selectionBackground": "#E8EBE4",
    "editor.selectionHighlightBackground": "#DDE1D8",
    "editorCursor.foreground": "#7A8470",
    "editorWhitespace.foreground": "#D0D5C8",
    "editorIndentGuide.background": "#DDE1D8",
    "editorLineNumber.foreground": "#B8C0AD",
    "editorLineNumber.activeForeground": "#7A8470",
    "sideBar.background": "#F0F2ED",
    "sideBar.foreground": "#5C6655",
    "activityBar.background": "#E8EBE4",
    "activityBar.foreground": "#434B3F",
    "statusBar.background": "#7A8470",
    "statusBar.foreground": "#F7F8F6",
    "titleBar.activeBackground": "#F0F2ED",
    "titleBar.activeForeground": "#434B3F",
    "panel.background": "#F0F2ED",
    "terminal.background": "#F7F8F6",
    "terminal.foreground": "#434B3F"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#B8C0AD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#5C6655",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#7A8470"
      }
    },
    {
      "scope": ["number"],
      "settings": {
        "foreground": "#9FAA93"
      }
    },
    {
      "scope": ["function"],
      "settings": {
        "foreground": "#434B3F",
        "fontStyle": "bold"
      }
    }
  ]
}